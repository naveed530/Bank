@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>RemoveAccount</title>

    <link rel="stylesheet" href="~/Content/layout.css" type="text/css" media="screen" />
    <!--[if lt IE 9]>
    <link rel="stylesheet" href="css/ie.css" type="text/css" media="screen" />
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <script src="~/Content/bootstrap.js" type="text/javascript"></script>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />

    <script src="~/Scripts/jquery-1.5.2.min.js" type="text/javascript"></script>
    <script src="~/Scripts/hideshow.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.tablesorter.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Scripts/jquery.equalHeight.js"></script>

</head>
<body>
    <div>
        <div class="col-lg-9">
            <table class="table" style="border:solid; margin-top:20px;">
                <thead>
                    <tr class="btn-primary" style="text-align:center">
                        <th>Delete</th>
                        <th>Account Number</th>
                        <th>Name</th>
                        <th>Address</th>
                        <th>Phone Number</th>
                        <th>Amount</th>
                        <th>Type</th>
                    </tr>
                </thead>

                @foreach (var obj in Model)
                {
                        <tr>
                            <td><a href="/AdminControl/DeleteAccount/@obj.AccountNo">Delete</a></td>
                            <td>@obj.AccountNo</td>
                            <td>@obj.name</td>
                            <td>@obj.address</td>
                            <td>@obj.phoneNo</td>
                            <td>@obj.amount</td>
                            <td>@obj.accountType</td>

                        </tr>
                }
            </table>
        </div>
    </div>
</body>
</html>
